<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<fx:root prefHeight="600.0" prefWidth="750.0" type="AnchorPane" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.view.MenuPanelController">
   <children>
      <VBox prefHeight="465.0" prefWidth="255.0">
         <children>
            <Button fx:id="NewPlayerButton" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" onMouseClicked="#gotoAddNewUserPanel" prefHeight="80.0" prefWidth="230.0" text="New Player">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="230.0" text="User Profile">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="230.0" text="Exit">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</fx:root>
