<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.MenuController">
   
   <children>
      <VBox layoutX="15.0" layoutY="20.0" prefHeight="580.0" prefWidth="240.0">
         <children>
            <Button fx:id="newplayer" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" onAction="#adduser" prefHeight="80.0" prefWidth="230.0" text="New Player">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="userprofile" mnemonicParsing="false" onAction="#showuser" prefHeight="80.0" prefWidth="230.0" text="User Profile">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
            <Button fx:id="exit" mnemonicParsing="false" onAction="#exit" prefHeight="80.0" prefWidth="230.0" text="Exit">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</AnchorPane>
