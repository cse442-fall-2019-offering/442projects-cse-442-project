<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>


<AnchorPane prefHeight="600.0" prefWidth="750.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
fx:controller = "game.menucontroller">
   <children>
      <VBox>
         <children>
            <Button fx:id = "newplayer" onAction = "#adduser" layoutX="10.0" layoutY="20.0" mnemonicParsing="false" prefHeight="80.0" prefWidth="230.0" text="New Player">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
            </Button>
            <Button fx:id = "userprofile" onAction = "#showuser" mnemonicParsing="false" prefHeight="80.0" prefWidth="230.0" text="User Profile">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
            <Button mnemonicParsing="false" prefHeight="80.0" prefWidth="230.0" text="Exit">
               <VBox.margin>
                  <Insets top="30.0" />
               </VBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
      </VBox>
   </children>
</fx:root>
